{
    "name": "toggl-connector",
    "version": "0.0.1",
    "description": "Toggl Connector for Google Data Studio",
    "main": "main.ts",
    "fileOrder": [
        "gds-types.ts",
        "setup.ts",
        "helpers.ts",
        "toggl-types.ts",
        "toggl.ts",
        "main.ts",
        "fields.ts",
        "auth.ts"
    ],
    "scripts": {
        "test": "jest",
        "prep-ts": "node prep-ts.js",
        "run-tsc": "tsc",
