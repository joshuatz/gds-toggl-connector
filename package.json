    "author": "Joshua Tzucker",
    "dependencies": {
        "@types/google-apps-script": "0.0.51",
        "es6-object-assign": "^1.1.0",
        "es6-promise": "^4.2.8",
        "typescript": "^3.5.1"
    },
    "devDependencies": {
        "@types/jest": "^24.0.15",
        "concat": "^1.0.3",
        "fs-extra": "^8.0.1",
        "jest": "^24.8.0",
        "replace-in-file": "^4.1.0",
        "ts-jest": "^24.0.2"
    }
}
